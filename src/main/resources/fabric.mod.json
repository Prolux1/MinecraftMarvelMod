{
  "schemaVersion": 1,
  "id": "marvel",
  "version": "${version}",
  "name": "Marvel",
  "description": "Mod inspired by the Universe of Marvel",
  "authors": [
    "Prolux"
  ],
  "contact": {},
  "license": "All-Rights-Reserved",
  "icon": "assets/marvel/icon.png",
  "environment": "*",
  "entrypoints": {
